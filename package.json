{
  "name": "hangedman-phrasal-flashcards",
  "version": "0.0.5",
  "description": "React starter project with sass and bootstrap",
  "main": "src/main.js",
  "scripts": {
    "compile": "webpack",
    "start": "node server.js",
    "watch-sass": "webpack --watch",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "verbose": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(jest))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "minimal",
    "starter"
  ],
  "author": "Ela Lach samandera",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/samandera/hangedman.git"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "file-loader": "^0.11.2",
    "img-loader": "^2.0.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "regenerator-runtime": "^0.10.5",
    "sass-loader": "^6.0.6",
    "webpack": "^2.6.1"
  }
}
